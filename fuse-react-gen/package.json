{
  "name": "@fuselab/react-gen",
  "version": "1.6.8",
  "description": "code gen tool for react.js in typescript",
  "main": "./lib/cli.js",
  "bin": {
    "fuse-gen": "./lib/cli.js"
  },
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "build-all": "tsc",
    "build-prod": "tsc",
    "tslint": "tslint -c tslint.json -t stylish --project tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/fuse-webui.git",
    "directory": "fuse-react-gen"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/*.spec.(ts|tsx)"
    ],
    "testPathIgnorePatterns": [
      "/examples/"
    ],
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "testURL": "http://localhost/"
  },
  "dependencies": {
    "@fuselab/ui-shared": "^1.1.0",
    "chalk": "^2.4.2",
    "glob": "^7.1.3",
    "graceful-fs": "4.1.15",
    "inquirer": "^6.2.1",
    "rimraf": "^2.6.2",
    "underscore": "^1.8.3",
    "yargs": "^15.2.0",
    "yargs-parser": "^18.1.0"
  },
  "devDependencies": {
    "@fuselab/build-config": "^1.0.0",
    "@types/inquirer": "^0.0.43",
    "@types/node": "12.12.1",
    "@types/underscore": "^1.8.7",
    "@types/yargs": "15.0.4",
    "awesome-typescript-loader": "5.2.1",
    "html-webpack-plugin": "3.2.0",
    "jest": "25.1.0",
    "merge": "1.2.1",
    "original": "1.0.2",
    "tslint": "5.15.0",
    "tslint-microsoft-contrib": "~5.0.2",
    "tslint-react": "~3.4.0",
    "typescript": "3.7.3",
    "uglify-js": "3.4.9",
    "uglifyjs-webpack-plugin": "2.1.1",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.3.1"
  },
  "author": "hailiu@gmail.com",
  "license": "ISC",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
