{
  "name": "{{package}}",
  "aadAppId": "12916cf1-b629-4ddf-b699-5a7d3035b38e",
  "version": "{{version}}",
  "description": "cli app for {{cli}} ",
  "main": "./lib/cli.js",
  "bin": {
    "{{cli}}": "./lib/cli.js"
  },
  "scripts": {
    "test": "jest --forceExit",
    "build": "tsc",
    "build-all": "tsc",
    "build-prod": "tsc",
    "tslint": "tslint -c tslint.json -t stylish --project tsconfig.json"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/*.spec.(ts|tsx)"
    ],
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "testURL": "http://localhost"
  },
  "license": "MIT",
  "dependencies": {
    "@fuselab/ui-shared": "~1.5.3",
    "chalk": "^2.4.2",
    "glob": "^7.1.3",
    "graceful-fs": "4.1.15",
    "inquirer": "^6.2.1",
    "rimraf": "^2.6.2",
    "underscore": "^1.8.3",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "@fuselab/build-config": "^1.0.0",
    "@types/inquirer": "^0.0.43",
    "@types/jasmine": "^2.8.6",
    "@types/node": "12.12.1",
    "@types/underscore": "^1.8.7",
    "awesome-typescript-loader": "5.2.1",
    "html-webpack-plugin": "3.2.0",
    "jasmine": "^2.9.0",
    "jest": "^22.1.4",
    "merge": "1.2.1",
    "original": "1.0.2",
    "ts-jest": "~22.0.1",
    "tslint": "5.12.1",
    "tslint-microsoft-contrib": "~5.0.2",
    "tslint-react": "~3.4.0",
    "typescript": "3.7.3",
    "webpack": "4.29.0",
    "webpack-cli": "^3.2.1"
  }
}
